{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "react/no-unescaped-entities": [
      "error",
      {
        "forbid": [
          {
            "char": "'",
            "alternative": "&apos;"
          },
          {
            "char": "\"",
            "alternative": "&quot;"
          },
          {
            "char": ">",
            "alternative": "&gt;"
          },
          {
            "char": "}",
            "alternative": "&#125;"
          }
        ]
      }
    ]
  }
}
